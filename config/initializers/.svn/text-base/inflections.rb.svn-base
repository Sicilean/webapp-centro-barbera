ActiveSupport::Inflector.inflections do |inflect|
  inflect.irregular 'prova', 'prove'
  inflect.irregular 'matrice', 'matrici'
  inflect.irregular 'tipologia', 'tipologie'
  inflect.irregular 'variabile', 'variabili'
  inflect.irregular 'cliente', 'clienti'
  inflect.irregular 'campione', 'campioni'
  inflect.irregular 'risultato', 'risultati'
  inflect.irregular 'parametro', 'parametri'
  inflect.irregular 'rapporto', 'rapporti'
  inflect.irregular 'fattura', 'fatture'
end
